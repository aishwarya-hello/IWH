<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hp.technowomen.Qualification">

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="14dp"
        android:visibility="visible"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/textView"
        android:layout_marginStart="21dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="10th %"
        android:layout_below="@+id/spinner"
        android:layout_alignStart="@+id/spinner" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/editText"
        android:layout_alignStart="@+id/editText" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/spinner2"
        android:layout_below="@+id/spinner2"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="12th %" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Graduation %"
        android:layout_below="@+id/spinner4"
        android:layout_alignStart="@+id/spinner3"
        android:layout_marginTop="21dp" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/editText11"
        android:layout_alignStart="@+id/editText11" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="graduation details"
        android:layout_above="@+id/button"
        android:layout_alignStart="@+id/editText12"
        android:layout_marginBottom="12dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/spinner"
        android:layout_marginStart="18dp"
        android:text="10th pass??" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/textView"
        android:layout_alignTop="@+id/spinner2"
        android:text="12th pass?" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/textView2"
        android:layout_alignTop="@+id/spinner3"
        android:text="Stream" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/spinner3"
        android:layout_alignStart="@+id/spinner3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/textView3"
        android:layout_alignTop="@+id/spinner4"
        android:text="Graduation??" />

    <Button
        android:id="@+id/button"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/textView5"
        android:text="Save" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/spinner"
        android:layout_alignEnd="@+id/textView"
        android:layout_marginBottom="145dp"
        android:layout_marginEnd="73dp"
        android:text="Button" />

</RelativeLayout>
